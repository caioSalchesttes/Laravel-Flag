    "autoload": {
        "psr-4": {
            "Caiosalchesttes\\LaravelFlag\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Caio .O",
            "email": "caiossxdgamer@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caiosalchesttes\\LaravelFlag\\Schema\\FlagMacro"
            ]
        }
    },
    "minimum-stability": "dev"
}